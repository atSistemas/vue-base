{
  "presets": [
    "env",
    "stage-2",
    "es2015"
    ],
  "plugins": ["transform-runtime"],
  "comments": false,
  "env": {
    "development": {
      "plugins": [
        ["babel-plugin-webpack-alias", { "config": "./webpack/index.babel.js" } ],
        "syntax-dynamic-import"
      ]
    },
    "production": {
      "plugins": [
        ["babel-plugin-webpack-alias", { "config": "./webpack/index.babel.js" } ],
        "syntax-dynamic-import"
      ]
    }
  }
}
